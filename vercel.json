

{
  "builds": [{
      "src": "jay/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": { "maxLambdaSize": "15mb" }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/"
    },
    {
      "src": "/(.*)",
      "dest": "<jay>/wsgi.py"
    }
  ],

  "outputDirectory": "static"
}